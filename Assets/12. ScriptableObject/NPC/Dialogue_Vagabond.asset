  startNode: start
  nodes:
  - id: start
    speakerName: "\uBC29\uB791\uC790"
    text: "\uAE38\uC744 \uBB3B\uB294\uB2E4\uACE0 \uAE38\uC774 \uC0DD\uAE30\uC9C4
      \uC54A\uC9C0. \uADF8\uB798\uB3C4 \uBB3B\uB294 \uC790\uC5D0\uAC8C \uAE38\uC774
      \uBCF4\uC774\uAE34 \uD558\uB354\uB77C. \uBB34\uC5C7\uC774 \uAD81\uAE08\uD55C\uAC00?"
    choices:
    - label: "\uC5B4\uB460/\uB3D9\uAD74\uC758 \uC608\uC758"
      next: talk_cave
    - label: "\uBCF4\uC2A4 \uAD6C\uC5ED\uC758 \uC804\uC870"
      next: talk_boss
    - label: "\uB3C4\uB9DD\uACFC \uC120\uD0DD\uC758 \uBBF8\uD559"
      next: talk_escape
    - label: "\uD754\uC801\uACFC \uAE30\uC5B5\uC758 \uBC95\uCE59"
      next: talk_trace
    next: end
  - id: talk_cave
    speakerName: "\uBC29\uB791\uC790"
    text: "\uC5B4\uB460\uC740 \uCE5C\uC808\uD574. \uB124 \uC2E4\uC218\uB3C4 \uAC10\uCDB0\uC8FC\uAC70\uB4E0.
      \uBB38\uC81C\uB294 \uB110 \uD3EC\uD568\uD574\uC11C \uAC10\uCD98\uB2E4\uB294
      \uAC70\uC9C0. \uD683\uBD88\uC740 \uB208\uC774 \uC544\uB2C8\uB77C \uB9C8\uC74C\uC744
      \uBC1D\uD78C\uB2E4 \uAC81\uC774 \uC904\uBA74 \uBC1C\uAC78\uC74C\uC774 \uB35C
      \uC2DC\uB044\uB7EC\uC6CC."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_boss
    speakerName: "\uBC29\uB791\uC790"
    text: "\uBC14\uB2E5\uC774 \uC7A0\uAE50 \uBA4E\uACE0, \uACF5\uAE30\uAC00 \uB108\uBB34
      \uC870\uC6A9\uD574\uC9C0\uBA74 \uBB38\uD131\uC774\uB780 \uB73B\uC774\uC57C.
      \uBB38\uD131\uC740 \uC608\uC758\uAC00 \uD544\uC694\uD558\uC9C0. \uBB3C\uB7EC\uC11C
      \uD55C\uC228 \uC26C\uACE0, \uC900\uBE44\uAC00 \uC778\uC0AC\uB2E4. \uC900\uBE44
      \uC5C6\uB294 \uC6A9\uAE30\uB294 \uC18C\uC74C\uC77C \uBFD0."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_escape
    speakerName: "\uBC29\uB791\uC790"
    text: "\uBE60\uB978 \uBC1C\uC740 \uD2C0\uB9B0 \uC120\uD0DD\uC744 \uC9C0\uC6CC\uC900\uB2E4.
      \uC6A9\uAC10\uD568\uC740 \uC2DC\uC791\uC744 \uB9CC\uB4E4\uACE0, \uAC81\uC740
      \uB0B4\uC77C\uC744 \uB9CC\uB4E0\uB2E4. \uC0B4\uC544\uB0A8\uC740 \uC790\uB9CC
      \uB2E4\uC74C \uC120\uD0DD\uC9C0\uB97C \uBCF8\uB2E4 \uADF8\uAC8C \uC774 \uB545\uC758
      \uADDC\uCE59\uC774\uC57C."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_trace
    speakerName: "\uBC29\uB791\uC790"
    text: "\uBC1C\uC790\uAD6D\uC774 \uB9CE\uC544\uC9C0\uBA74 \uAE30\uC5B5\uB3C4 \uB9CE\uC544\uC9C4\uB2E4.
      \uB124 \uB0C4\uC0C8\uB294 \uBC14\uB78C\uC744 \uD0C0\uACE0, \uB108\uC758 \uC2E4\uC218\uB294
      \uC18C\uBB38\uC744 \uD0C0\uC9C0. \uC18C\uB9AC\uB294 \uC9E7\uACE0 \uB0C4\uC0C8\uB294
      \uAE38\uB2E4 \uC228 \uC26C\uB294 \uBC95\uBD80\uD130 \uBC14\uAFD4\uBCF4\uB77C."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: end
    speakerName: "\uBC29\uB791\uC790"
    text: "\uAE38\uC774 \uB458\uC774\uBA74 \uC870\uC6A9\uD55C \uCABD\uC744. \uC870\uC6A9\uD55C
      \uAE38\uC5D4 \uC0B4\uC544\uB0A8\uC740 \uC774\uB4E4\uC758 \uBC1C\uC790\uAD6D\uC774
      \uC788\uB2E4.\n\uC0B4\uC544 \uB0A8\uC544\uB77C."
    choices: []
