  m_Script: {fileID: 11500000, guid: 83c5c5a22be6f8c4ea130acca2554ba7, type: 3}
  m_Name: Dialogue_Hunter
  m_EditorClassIdentifier: 
  startNode: start
  nodes:
  - id: start
    speakerName: "\uD5CC\uD130"
    text: "\uCC98\uC74C \uBCF4\uB294 \uC5BC\uAD74\uC774\uB124. \uD45C\uC815 \uBCF4\uC544\uD558\uB2C8
      \uBC24\uC740 \uC544\uC9C1 \uC548 \uACAA\uC5B4\uBD24\uAD70. \uAC71\uC815 \uB9C8,
      \uB2E4\uB4E4 \uD55C \uBC88\uCBE4\uC740 \uAE5C\uAE5C\uC774\uB97C \uCE5C\uAD6C\uB85C
      \uB454\uB2E4\uB2C8\uAE4C. \uBB58 \uC598\uAE30\uD574\uC904\uAE4C?"
    choices:
    - label: "\uC18C\uB9AC \uB0AE\uCD94\uB294 \uC694\uB839"
      next: talk_sound
    - label: "\uBC24 \uB300\uBE44 & \uBD88 \uAD00\uB9AC"
      next: talk_night
    - label: "\uD5C8\uAE30/\uC2A4\uD0DC\uBBF8\uB098 \uAD00\uB9AC"
      next: talk_hunger
    - label: "\uBC14\uB78C & \uC0AC\uB0E5 \uB3D9\uC120"
      next: talk_wind
    next: end
  - id: talk_sound
    speakerName: "\uD5CC\uD130"
    text: "\uC0AC\uB0E5\uAC10\uC740 \uADC0\uAC00 \uBC1D\uC544. \uB2EC\uB9B4 \uB550
      \uBC1C\uC790\uAD6D\uC774 \uBA3C\uC800 \uB4E4\uD0A4\uC9C0. \uCC9C\uCC9C\uD788
      \uAC77\uACE0, \uD480 \uB364\uBD88\uC740 \uD53C\uD558\uACE0, \uB3CC\uC740 \uBC1F\uC9C0
      \uB9C8. \uC2E0\uBC1C\uC5D0 \uC9C4\uD759 \uC870\uAE08 \uBB3B\uD788\uBA74 \uC18C\uB9AC\uB3C4
      \uC870\uAE08 \uBB3B\uD600\uC9C0\uC9C0."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_night
    speakerName: "\uD5CC\uD130"
    text: "\uBC24\uC5D4 \uBD88\uC774 \uCE5C\uAD6C\uC774\uC790 \uAD11\uACE0\uD310\uC774\uC57C.
      \uBD88\uC744 \uB192\uAC8C \uD0A4\uC6B0\uBA74 \uB530\uB73B\uD558\uC9C0\uB9CC,
      \uBA40\uB9AC\uC11C\uB3C4 \uBC18\uC9DD\uC774\uC9C0. \uBC14\uB78C \uB4F1\uC9C0\uACE0
      \uC791\uC740 \uBD88\uBD80\uD130 \uBD99\uC5EC. \uC816\uC740 \uC7A5\uC791\uC740
      \uC58C\uC804\uD788 \uB9C8\uB978 \uAC00\uC9C0\uB85C \uC124\uB4DD\uD558\uACE0."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_hunger
    speakerName: "\uD5CC\uD130"
    text: "\uBC30\uACE0\uD514\uC740 \uC870\uC6A9\uD788 \uB2E4\uAC00\uC640. \uD5C8\uAE30\uAC00
      30%\uCBE4 \uB0A8\uC558\uC744 \uB54C \uAC04\uB2E8\uD788\uB77C\uB3C4 \uBA39\uC5B4.
      \uADF8\uB54C \uD68C\uBCF5\uD558\uB294 \uAC8C, \uC4F0\uB7EC\uC9C4 \uB2E4\uC74C
      \uC77C\uC5B4\uB098\uB294 \uAC83\uBCF4\uB2E4 \uD6E8\uC52C \uC2F8\uAC8C \uBA39\uD600."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_wind
    speakerName: "\uD5CC\uD130"
    text: "\uBC14\uB78C\uC740 \uACF5\uC9DC \uC548\uB0B4\uC790\uC57C. \uBC14\uB78C
      \uBD88\uC5B4\uC624\uB294 \uCABD\uC73C\uB85C \uC11C \uC788\uC73C\uBA74 \uB124
      \uB0C4\uC0C8\uAC00 \uBA3C\uC800 \uB3C4\uCC29\uD574. \uC0AC\uB0E5\uC740 \uBC14\uB78C\uC744
      \uB4F1\uC9C0\uACE0, \uB3C4\uB9DD\uCE60 \uB550 \uBC14\uB78C\uC744 \uAC70\uC2AC\uB7EC.
      \uAC04\uB2E8\uD558\uC9C0?"
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: end
    speakerName: "\uD5CC\uD130"
    text: "\uADF8\uB798, \uC624\uB298\uC740 \uC5EC\uAE30\uAE4C\uC9C0. \uBC1C\uC790\uAD6D\uC740
      \uAC00\uBCCD\uAC8C, \uBC30\uB0AD\uC740 \uB354 \uAC00\uBCCD\uAC8C."
    choices: []
    next: 
