  startNode: start
  nodes:
  - id: start
    speakerName: "\uC7A5\uC778"
    text: "\uC9D1 \uC9D3\uB294 \uC7AC\uBBF8\uB97C \uC544\uB098? \uBE44\uB97C \uB9C9\uACE0,
      \uBC14\uB78C\uC744 \uB9C9\uACE0, \uADFC\uC2EC\uC744 \uB9C9\uC9C0. \uC624\uB298\uC740
      \uBB58\uB85C \uB124 \uADFC\uC2EC\uC744 \uB9C9\uC544\uC904\uAE4C?"
    choices:
    - label: "\uAE30\uB465\xB7\uBAA8\uC11C\uB9AC\uC758 \uD575\uC2EC"
      next: talk_pillar
    - label: "\uC9C0\uBD95\uACFC \uBE57\uBB3C\uC758 \uACFC\uD559\""
      next: talk_roof
    - label: "\uBB38\xB7\uCC3D \uBC30\uCE58 \uAC10\uAC01"
      next: talk_doorwin
    - label: "\uB3C4\uAD6C \uAD00\uB9AC & \uB0B4\uAD6C\uB3C4"
      next: talk_tools
    next: end
  - id: talk_pillar
    speakerName: "\uC7A5\uC778"
    text: "\uBAA8\uC11C\uB9AC\uB294 \uC9D1\uC758 \uC5B4\uAE68\uC57C. \uAE30\uB465\uC744
      \uBA3C\uC800 \uC138\uC6B0\uACE0, \uADF8 \uC0AC\uC774\uB97C \uBCBD\uC774 \uCC44\uC6CC.
      \uCF54\uB108\uAC00 \uB611\uBC14\uB974\uBA74 \uC9D1\uC774 \uB611\uBC14\uB85C
      \uC11C\uACE0, \uC7A0\uB3C4 \uB611\uBC14\uB85C \uC794\uB2E4. \uC090\uB531\uD55C
      \uC7A0\uC740 \uD5C8\uB9AC\uC5D0\uAC8C \uBE44\uC2FC \uCCAD\uAD6C\uC11C\uAC00
      \uC624\uAC70\uB4E0."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_roof
    speakerName: "\uC7A5\uC778"
    text: "\uC9C0\uBD95\uC740 \uC6B0\uC0B0\uC774 \uC544\uB2C8\uB2E4. \uC6B0\uC0B0\uC740
      \uB108\uB9CC \uC9C0\uD0A4\uC9C0\uB9CC, \uC9C0\uBD95\uC740 \uB124 \uBAA8\uB4E0
      \uAC78 \uC9C0\uCF1C. \uACBD\uC0AC\uB97C \uC544\uB07C\uC9C0 \uB9D0\uACE0, \uBE57\uBB3C
      \uAE38\uC744 \uB9CC\uB4E4\uC5B4. \uBB3C\uC740 \uACE0\uC9D1\uC774 \uC138\uC9C0\uB9CC
      \uAE38\uC774 \uC788\uC73C\uBA74 \uC58C\uC804\uD558\uC9C0."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_doorwin
    speakerName: "\uC7A5\uC778"
    text: "\uBB38\uC740 \uC2B5\uAD00\uC774\uACE0 \uCC3D\uC740 \uC131\uACA9\uC774\uC57C.
      \uBB38\uC740 \uB3D9\uC120\uACFC \uBC14\uB78C\uAE38\uC774 \uACB9\uCE58\uC9C0
      \uC54A\uAC8C, \uCC3D\uC740 \uBE5B\uC774 \uC6D0\uD558\uB294 \uC790\uB9AC\uC5D0.
      \uD574\uAC00 \uB4DC\uB294 \uC2DC\uAC04\uD45C\uB97C \uAC00\uAD6C\uAC00 \uC88B\uC544\uD574.
      \uC18C\uD30C\uB294 \uD587\uC0B4\uACFC \uCE5C\uD55C \uCE5C\uAD6C\uAC70\uB4E0."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: talk_tools
    speakerName: "\uC7A5\uC778"
    text: "\uBB34\uB38C\uC9C4 \uD1B1\uC740 \uB098\uBB34\uBCF4\uB2E4 \uB108\uB97C
      \uBA3C\uC800 \uBCA0\uC5B4. \uB0B4\uAD6C\uB3C4 20%\uCBE4 \uB0A8\uC558\uC744
      \uB54C \uAC08\uC544\uC918. \uAC08\uC544\uC8FC\uB294 10\uCD08\uAC00 \uBB34\uB9AC\uD55C
      10\uBD84\uC744 \uC774\uACA8. \uACF5\uAD6C\uD55C\uD14C \uCE5C\uC808\uD558\uBA74
      \uACF5\uAD6C\uAC00 \uB124 \uC190\uBAA9\uC5D0 \uCE5C\uC808\uD558\uAC70\uB4E0."
    choices:
    - label: "\uB2E4\uB978 \uC598\uAE30\uB3C4 \uD574\uC918"
      next: start
    - label: "\uC774\uB9CC\uD560\uAC8C"
      next: end
    next: end
  - id: end
    speakerName: "\uC7A5\uC778"
    text: "\uC88B\uC740 \uC9D1\uC740 \uC18C\uC74C\uC744 \uC904\uC774\uACE0, \uB098\uC05C
      \uC9D1\uC740 \uC5B8\uC131\uC744 \uB192\uC5EC. \uC6B0\uB9B0 \uC804\uC790\uB97C
      \uC9D3\uC790\uACE0."
    choices: []
